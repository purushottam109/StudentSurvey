<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
 <managed-bean>
  <managed-bean-name>studentSurvey</managed-bean-name>
  <managed-bean-class>com.swe645.assign5.StudentSurvey</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>/index.html</from-view-id>
  <navigation-case>
   <from-outcome>StudentSurveyPage</from-outcome>
   <to-view-id>/StudentSurveyPage.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>ListSurvey</from-outcome>
   <to-view-id>/ListSurvey.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/StudentSurveyPage.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>index</from-outcome>
   <to-view-id>/index.html</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>WinnerAcknowledgement</from-outcome>
   <to-view-id>/WinnerAcknowledgement.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>SimpleAcknowledgement</from-outcome>
   <to-view-id>/SimpleAcknowledgement.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application/>
</faces-config>
