<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from SearchServiceService WSDL service -->

  <service name="SearchService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://example.sample.com"/>
      <parameter name="wsdlServiceElement" value="SearchServiceService"/>
      <parameter name="schemaQualified" value="http://example.sample.com"/>
      <parameter name="wsdlServicePort" value="SearchService"/>
      <parameter name="className" value="com.sample.example.SearchServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="SearchService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="search" qname="operNS:search" xmlns:operNS="http://example.sample.com" returnQName="retNS:searchReturn" xmlns:retNS="http://example.sample.com" returnType="rtns:ArrayOf_xsd_anyType" xmlns:rtns="http://example.sample.com" returnItemQName="tns:item" xmlns:tns="http://example.sample.com" soapAction="" >
        <parameter qname="pns:fName" xmlns:pns="http://example.sample.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:lName" xmlns:pns="http://example.sample.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:city" xmlns:pns="http://example.sample.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:state" xmlns:pns="http://example.sample.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="search"/>

      <arrayMapping
        xmlns:ns="http://example.sample.com"
        qname="ns:ArrayOf_xsd_anyType"
        type="java:java.lang.Object[]"
        innerType="cmp-ns:anyType" xmlns:cmp-ns="http://www.w3.org/2001/XMLSchema"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://example.sample.com"
        qname="ns:>search"
        type="java:com.sample.example.Search"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://example.sample.com"
        qname="ns:>searchResponse"
        type="java:com.sample.example.SearchResponse"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
